{
  "name": "fibroguardian",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "depcheck": "depcheck --json",
    "tsprune": "ts-prune",
    "cline:setup": "node scripts/cline-setup.js setup",
    "cline:test": "node scripts/cline-setup.js test",
    "cline:reset": "node scripts/cline-setup.js reset",
    "cline:context": "node scripts/cline-setup.js context",
    "db:types": "supabase gen types typescript --local",
    "db:validate": "tsc --noEmit",
    "db:optimize": "node scripts/optimize-database.js",
    "db:optimize:prod": "cross-env NODE_ENV=production node scripts/optimize-database.js",
    "db:migrate": "supabase migration up",
    "check:memory-leaks": "node scripts/check-memory-leaks.js",
    "// Code Quality Scripts": "",
    "fix": "npm run fix:lint && npm run fix:format && npm run fix:imports",
    "fix:lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "fix:format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,yml,yaml}\"",
    "fix:imports": "organize-imports-cli \"**/*.{ts,tsx}\"",
    "fix:all": "npm run fix && npm run fix:tailwind && npm run fix:types",
    "fix:tailwind": "eslint . --ext .ts,.tsx,.js,.jsx --fix --rule \"tailwindcss/classnames-order: error\" --rule \"tailwindcss/enforces-shorthand: error\"",
    "fix:types": "node scripts/fix-typescript.js",
    "// Auto-Fix Scripts": "",
    "autofix": "node scripts/auto-fix-system.js",
    "autofix:watch": "node scripts/realtime-fixer.js",
    "autofix:setup": "node scripts/realtime-fixer.js --setup",
    "autofix:now": "node scripts/realtime-fixer.js --fix-now",
    "autofix:db": "node scripts/auto-fix-system.js && node scripts/fix-typescript.js",
    "autofix:types": "scripts/cron/auto-fix-typescript.bat",
    "autofix:types:unix": "bash scripts/cron/auto-fix-typescript.sh",
    "autofix:learn": "node scripts/auto-learning-system.js",
    "autofix:assertions": "node scripts/fix-type-assertions.js",
    "autofix:advanced": "node scripts/advanced-type-fixer.js",
    "autofix:specific": "node scripts/fix-specific-errors.js",
    "autofix:enhanced": "node scripts/enhanced-specific-errors.js",
    "autofix:unified": "node scripts/unified-auto-fix.js",
    "autofix:all": "npm run autofix:unified",
    "// Quality Reports": "",
    "quality:report": "npm run quality:eslint && npm run quality:types && npm run quality:complexity",
    "quality:eslint": "eslint . --ext .ts,.tsx,.js,.jsx --format json > reports/eslint-report.json || true",
    "quality:types": "tsc --noEmit --pretty false > reports/typescript-report.txt 2>&1 || true",
    "quality:complexity": "npx complexity-report -o reports/complexity.json src/ || true"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-devtools": "^5.76.1",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "critters": "^0.0.23",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.511.0",
    "next": "^14.2.29",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "react-intersection-observer": "^9.5.3",
    "recharts": "^2.15.3",
    "sharp": "^0.33.2",
    "stripe": "^18.1.0",
    "uuid": "^9.0.1",
    "web-vitals": "^5.0.1",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.4.1"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.2",
    "@babel/traverse": "^7.27.1",
    "@next/bundle-analyzer": "^14.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.0.1",
    "chokidar": "^3.5.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.0",
    "next-pwa": "^5.6.0",
    "organize-imports-cli": "^0.10.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "stylelint": "^16.19.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^3.3.0",
    "ts-prune": "^0.10.3",
    "tsx": "^4.19.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run db:validate"
    }
  }
}
