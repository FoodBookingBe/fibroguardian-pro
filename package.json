{
  "name": "fibroguardian-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --coverage",
    "cypress": "cypress open",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "audit:dead-code": "node scripts/find-dead-code.js",
    "audit:components": "ts-node scripts/audit-components.ts",
    "audit:all": "npm run audit:dead-code && npm run audit:components",
    "analyze": "ANALYZE=true next build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-devtools": "^5.76.1",
    "@upstash/redis": "^1.22.0",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "localforage": "^1.10.0",
    "lucide-react": "^0.511.0",
    "next": "^14.2.28",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
