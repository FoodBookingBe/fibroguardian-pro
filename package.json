    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/parser": "^8.32.1",
    "autocannon": "^8.0.0",
    "autoprefixer": "^10.4.16",
    "critters": "^0.0.23",
    "cypress": "^13.5.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "glob": "^11.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "sharp": "^0.34.1",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.2.2"
